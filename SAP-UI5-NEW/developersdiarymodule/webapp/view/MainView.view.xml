<mvc:View controllerName="ns.developersdiarymodule.controller.MainView" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
    <Shell>
        <App class="myAppDemoWT">
            <pages>
                <Page title="{i18n>homePageTitle}">
                    <headerContent>
                        <Button
                            icon="sap-icon://hello-world"
                            press="onOpenDialog"/>
                    </headerContent>
                    <content>
                       <mvc:XMLView 
                            viewName="ns.developersdiarymodule.view.HelloPanel"/>
                        <mvc:XMLView 
                            viewName="ns.developersdiarymodule.view.InvoiceList"/>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
